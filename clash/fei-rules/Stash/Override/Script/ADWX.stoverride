name: 微信公众号去广告
desc: 去除微信公众号文章内广告

http:
  mitm:
    - "mp.weixin.qq.com"
  script:
    - match: ^https?:\/\/mp\.weixin\.qq\.com\/mp\/getappmsgad
      name: wx
      type: response
      require-body: true
      timeout: 10
