name: Sub-Store
desc: 高级订阅管理工具

http:
  mitm:
    - "sub.store"
  script:
    - match: ^https?:\/\/sub\.store
      name: SubStore
      type: request
      require-body: true
      timeout: 120

script-providers:
  SubStore:
    url: https://raw.githubusercontent.com/Peng-YM/Sub-Store/master/backend/sub-store.min.js
    interval: 86400
